{
  "name": "ml-trial",
  "version": "0.0.0",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "dev": "npm start",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:staged": "lint-staged",
    "lint:all": "npm run lint && npm run lint:ls",
    "lint": "npm run lint:eslint -- 'app'",
    "lint:ls": "ls-lint",
    "lint:eslint": "eslint --cache --ext 'jsx,js'",
    "lint:fix": "npm run lint:eslint -- 'app' --fix",
    "create-icon": "node internal/scripts/create-icon.mjs",
    "collect-messages": "node internal/scripts/collect-messages.mjs",
    "formatjs-extract-compile": "formatjs extract 'app/translations/tmp-messages.js' --flatten --out-file 'app/translations/extracted/de.json' && formatjs compile 'app/translations/extracted/de.json' --out-file 'app/translations/de.json'",
    "remove-tmp-messages": "rimraf app/translations/tmp-messages.js",
    "extract-messages": "npm run collect-messages && npm run formatjs-extract-compile && npm run remove-tmp-messages",
    "pretest": "npm run test:clean && npm run lint:all",
    "test": "cross-env NODE_ENV=test TZ=UTC jest --coverage --verbose --config jest.config.json",
    "test:watch": "cross-env NODE_ENV=test TZ=UTC jest --coverage --verbose --config jest.config.json --watch",
    "test:clean": "rimraf ./coverage",
    "postinstall": "rimraf .eslintcache",
    "prebuild": "rimraf ./dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "changelog": "npx conventional-changelog -u | tail -n +3",
    "release": "release-it"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --ignore-unknown",
      "npm run lint:eslint -- --fix "
    ]
  },
  "dependencies": {
    "@faker-js/faker": "^7.2.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.6.4",
    "@material-icons/svg": "^1.0.28",
    "@popperjs/core": "^2.11.5",
    "@reduxjs/toolkit": "^1.8.2",
    "@szhsin/react-menu": "^3.1.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tanstack/react-location": "^3.7.4",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.3",
    "deep-freeze": "0.0.1",
    "focus-trap-react": "^9.0.0",
    "jsonapi-normalizer": "^2.0.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "qs": "^6.10.5",
    "react": "^17.0.2",
    "react-day-picker": "^8.0.7",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.32.0",
    "react-intl": "^6.0.4",
    "react-popper": "^2.3.0",
    "react-redux": "^8.0.2",
    "react-select": "^5.3.2",
    "react-toastify": "^9.0.4",
    "redux": "^4.2.0",
    "redux-first-history": "^5.0.9",
    "redux-injectors": "^2.0.0",
    "redux-saga": "^1.1.3",
    "rifm": "^0.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-react": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@formatjs/cli": "^5.0.2",
    "@ls-lint/ls-lint": "^1.11.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/react": "^6.5.8",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/deep-freeze": "^0.1.2",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/prop-types": "^15.7.5",
    "@types/react-select": "^5.0.1",
    "@types/redux-saga": "^0.10.5",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-formatjs": "^10.3.24",
    "babel-plugin-module-resolver": "^4.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-atomic-design-hierarchy": "^1.0.11",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "eslint-plugin-storybook": "^0.5.12",
    "gh-pages": "^5.0.0",
    "glob": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "redux-saga-test-plan": "^4.0.5",
    "release-it": "^15.0.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.2",
    "vite": "^2.9.12",
    "vite-aliases": "^0.9.2",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-svgr": "^2.1.0",
    "whatwg-fetch": "^3.6.2",
    "workbox-window": "^6.5.3"
  },
  "overrides": {
    "react-redux": "$react-redux"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
